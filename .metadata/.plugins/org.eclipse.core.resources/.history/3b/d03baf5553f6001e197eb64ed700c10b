package fr.metz.mns.tp.tpParisSportifs.repositories;

import org.hibernate.mapping.List;
import org.springframework.data.jpa.repository.JpaRepository;

import fr.metz.mns.tp.tpParisSportifs.models.Evenement;

public interface EvenementRepository extends JpaRepository<Evenement, Integer> {

    List<Evenement> findByType(String type);

    // Exemple de requête pour trouver les événements associés à un match donné
    List<Evenement> findByMatchId(int matchId);
}
